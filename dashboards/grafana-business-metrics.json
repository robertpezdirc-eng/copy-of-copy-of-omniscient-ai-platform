{
  "dashboard": {
    "title": "Business & ML Metrics Monitoring",
    "description": "Business metrics including ML predictions, revenue tracking, and feature usage",
    "tags": ["business", "ml", "revenue", "predictions"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
        "type": "stat",
        "title": "Total Revenue (24h)",
        "targets": [
          {
            "expr": "sum(increase(business_revenue_total_cents[24h])) / 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        }
      },
      {
        "id": 2,
        "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
        "type": "stat",
        "title": "Active Users",
        "targets": [
          {
            "expr": "sum(business_active_users)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value"
        }
      },
      {
        "id": 3,
        "gridPos": { "h": 6, "w": 6, "x": 12, "y": 0 },
        "type": "stat",
        "title": "ML Predictions (1h)",
        "targets": [
          {
            "expr": "sum(increase(ml_model_inference_total[1h]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value"
        }
      },
      {
        "id": 4,
        "gridPos": { "h": 6, "w": 6, "x": 18, "y": 0 },
        "type": "stat",
        "title": "Avg User Engagement",
        "targets": [
          {
            "expr": "avg(business_user_engagement_score)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "value": 0, "color": "red" },
            { "value": 40, "color": "yellow" },
            { "value": 70, "color": "green" }
          ]
        }
      },
      {
        "id": 5,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
        "type": "graph",
        "title": "Revenue by Tier",
        "targets": [
          {
            "expr": "sum by (tier) (rate(business_revenue_total_cents[5m])) / 100",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "currencyUSD", "label": "Revenue/sec" }
        ],
        "tooltip": { "shared": true },
        "legend": { "show": true }
      },
      {
        "id": 6,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 6 },
        "type": "graph",
        "title": "Revenue by Feature",
        "targets": [
          {
            "expr": "topk(10, sum by (feature) (rate(business_revenue_total_cents[5m]))) / 100",
            "legendFormat": "{{feature}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "currencyUSD", "label": "Revenue/sec" }
        ],
        "tooltip": { "shared": false }
      },
      {
        "id": 7,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
        "type": "graph",
        "title": "ML Model Inference Rate",
        "targets": [
          {
            "expr": "sum by (model_name, status) (rate(ml_model_inference_total[5m]))",
            "legendFormat": "{{model_name}} - {{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Inferences/sec" }
        ],
        "tooltip": { "shared": true }
      },
      {
        "id": 8,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 14 },
        "type": "graph",
        "title": "ML Model Accuracy",
        "targets": [
          {
            "expr": "ml_model_accuracy_percent",
            "legendFormat": "{{model_name}} v{{model_version}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percent", "min": 0, "max": 100, "label": "Accuracy %" }
        ],
        "tooltip": { "shared": true }
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 22 },
        "type": "graph",
        "title": "ML Prediction Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (model_name, le) (rate(ml_model_prediction_seconds_bucket[5m])))",
            "legendFormat": "{{model_name}} p95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, sum by (model_name, le) (rate(ml_model_prediction_seconds_bucket[5m])))",
            "legendFormat": "{{model_name}} p50",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Latency" }
        ],
        "tooltip": { "shared": true }
      },
      {
        "id": 10,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 22 },
        "type": "graph",
        "title": "Active Users by Tier",
        "targets": [
          {
            "expr": "business_active_users",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "label": "Users" }
        ],
        "tooltip": { "shared": true }
      },
      {
        "id": 11,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 30 },
        "type": "graph",
        "title": "Feature Usage Rate",
        "targets": [
          {
            "expr": "topk(10, sum by (feature_name) (rate(feature_usage_total[5m])))",
            "legendFormat": "{{feature_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Usage/sec" }
        ],
        "tooltip": { "shared": false }
      },
      {
        "id": 12,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 30 },
        "type": "graph",
        "title": "API Calls by Tenant",
        "targets": [
          {
            "expr": "topk(10, sum by (tenant_id, tier) (rate(api_calls_tenant_total[5m])))",
            "legendFormat": "{{tenant_id}} ({{tier}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Calls/sec" }
        ],
        "tooltip": { "shared": false }
      },
      {
        "id": 13,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 38 },
        "type": "graph",
        "title": "API Data Processed",
        "targets": [
          {
            "expr": "sum by (direction) (rate(api_data_processed_bytes_total[5m]))",
            "legendFormat": "{{direction}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "Bps", "label": "Bytes/sec" }
        ],
        "tooltip": { "shared": true }
      },
      {
        "id": 14,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 38 },
        "type": "graph",
        "title": "Business Errors by Type",
        "targets": [
          {
            "expr": "sum by (error_type, severity) (rate(business_errors_total[5m]))",
            "legendFormat": "{{error_type}} ({{severity}})",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "reqps", "label": "Errors/sec" }
        ],
        "tooltip": { "shared": false }
      },
      {
        "id": 15,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 46 },
        "type": "graph",
        "title": "User Engagement Score by Tier",
        "targets": [
          {
            "expr": "business_user_engagement_score",
            "legendFormat": "{{tier}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short", "min": 0, "max": 100, "label": "Engagement Score" }
        ],
        "tooltip": { "shared": true }
      },
      {
        "id": 16,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 46 },
        "type": "graph",
        "title": "Infrastructure Cost Estimate",
        "targets": [
          {
            "expr": "sum by (resource_type) (infrastructure_cost_estimate_usd_hour)",
            "legendFormat": "{{resource_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "currencyUSD", "label": "Cost/hour" }
        ],
        "tooltip": { "shared": true }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "tier",
          "type": "query",
          "query": "label_values(business_active_users, tier)",
          "label": "Tier",
          "multi": true,
          "includeAll": true
        },
        {
          "name": "model",
          "type": "query",
          "query": "label_values(ml_model_inference_total, model_name)",
          "label": "ML Model",
          "multi": true,
          "includeAll": true
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
