{
  "dashboard": {
    "title": "Redis Cache Monitoring",
    "description": "Comprehensive Redis cache monitoring including hit rates, latency, and resource usage",
    "tags": ["cache", "redis", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
        "type": "graph",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "percent", "min": 0, "max": 100 }
        ],
        "tooltip": { "shared": true },
        "legend": { "show": true }
      },
      {
        "id": 2,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
        "type": "stat",
        "title": "Current Hit Rate",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "textMode": "value_and_name"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "value": 0, "color": "red" },
            { "value": 50, "color": "yellow" },
            { "value": 80, "color": "green" }
          ]
        }
      },
      {
        "id": 3,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
        "type": "graph",
        "title": "Cache Operations Rate",
        "targets": [
          {
            "expr": "rate(cache_hits_total{cache_type='redis'}[5m])",
            "legendFormat": "Redis Hits/s",
            "refId": "A"
          },
          {
            "expr": "rate(cache_hits_total{cache_type='memory'}[5m])",
            "legendFormat": "Memory Hits/s",
            "refId": "B"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Misses/s",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "reqps" }
        ]
      },
      {
        "id": 4,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
        "type": "graph",
        "title": "Cache Size",
        "targets": [
          {
            "expr": "cache_size_items{cache_type='redis'}",
            "legendFormat": "Redis Items",
            "refId": "A"
          },
          {
            "expr": "cache_size_items{cache_type='memory'}",
            "legendFormat": "Memory Items",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "short" }
        ]
      },
      {
        "id": 5,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
        "type": "graph",
        "title": "Redis Memory Usage",
        "targets": [
          {
            "expr": "redis_memory_used_bytes",
            "legendFormat": "Used Memory",
            "refId": "A"
          },
          {
            "expr": "redis_memory_peak_bytes",
            "legendFormat": "Peak Memory",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "bytes" }
        ]
      },
      {
        "id": 6,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 16 },
        "type": "stat",
        "title": "Redis Connection Status",
        "targets": [
          {
            "expr": "redis_connected",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value_and_name"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            { "value": 0, "color": "red" },
            { "value": 1, "color": "green" }
          ]
        },
        "mappings": [
          { "value": 0, "text": "Disconnected" },
          { "value": 1, "text": "Connected" }
        ]
      },
      {
        "id": 7,
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
        "type": "graph",
        "title": "Redis Keys Total",
        "targets": [
          {
            "expr": "redis_keys_total",
            "legendFormat": "Total Keys",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short" }
        ]
      },
      {
        "id": 8,
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
        "type": "graph",
        "title": "Redis Connected Clients",
        "targets": [
          {
            "expr": "redis_connected_clients",
            "legendFormat": "Clients",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "short" }
        ]
      },
      {
        "id": 9,
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 32 },
        "type": "graph",
        "title": "Request Latency: Cached vs Uncached",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{path=~'/api/.*'}[5m]))",
            "legendFormat": "p95 All Requests",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{path=~'/api/.*'}[5m]))",
            "legendFormat": "p50 All Requests",
            "refId": "B"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Latency" }
        ],
        "tooltip": { "shared": true }
      }
    ],
    "templating": {
      "list": []
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
