{
  "uid": "omni-cloud-overview",
  "title": "OMNI Cloud Overview",
  "tags": ["OMNI", "Cloud", "Prometheus"],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "time": {"from": "now-6h", "to": "now"},
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "current": {"text": "Prometheus", "value": "PROMETHEUS_DS"}
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Revenue (Stripe)",
      "id": 1,
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [{"expr": "omni_revenue", "refId": "A"}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "values": false}, "orientation": "auto"}
    },
    {
      "type": "stat",
      "title": "Active Users",
      "id": 2,
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [{"expr": "omni_active_users", "refId": "A"}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "values": false}, "orientation": "auto"}
    },
    {
      "type": "stat",
      "title": "Requests (total)",
      "id": 3,
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [{"expr": "omni_requests_total", "refId": "A"}],
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "values": false}, "orientation": "auto"}
    },
    {
      "type": "stat",
      "title": "Uptime (%)",
      "id": 4,
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [{"expr": "omni_uptime_percent", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "percent"}},
      "options": {"reduceOptions": {"calcs": ["lastNotNull"], "values": false}, "orientation": "auto"}
    },
    {
      "type": "timeseries",
      "title": "Request Rate (req/s)",
      "id": 5,
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 6},
      "datasource": {"type": "prometheus", "uid": "${DS_PROMETHEUS}"},
      "targets": [{"expr": "rate(omni_requests_total[5m])", "refId": "A"}],
      "fieldConfig": {"defaults": {"unit": "reqps"}, "overrides": []}
    }
  ]
}