{
  "dashboard": {
    "title": "Omni Platform - Overview",
    "tags": ["omni", "platform", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100"
          }
        ],
        "type": "graph"
      },
      {
        "title": "API Request Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (method, path)"
          }
        ],
        "type": "graph"
      },
      {
        "title": "ML Predictions",
        "targets": [
          {
            "expr": "sum(rate(ml_predictions_total[5m])) by (model_type)"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Tenant API Usage",
        "targets": [
          {
            "expr": "sum(tenant_api_calls_total) by (tenant_id, tier)"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Response Time (p95)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))"
          }
        ],
        "type": "graph"
      },
      {
        "title": "Error Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m]))"
          }
        ],
        "type": "graph"
      }
    ],
    "schemaVersion": 36,
    "version": 1
  }
}
