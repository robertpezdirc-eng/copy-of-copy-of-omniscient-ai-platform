{
  "vr_config": {
    "version": "1.0.0",
    "name": "Omni VR Gateway",
    "description": "VR integration gateway for Omni platform with core integration",
    "enabled": true,
    "supported_apis": ["WebXR", "WebVR", "A-Frame"],
    "target_devices": ["Oculus Quest", "HTC Vive", "Mobile VR", "Desktop VR"],
    "auto_publish": true,
    "require_https": true,
    "core_integration": {
      "enabled": true,
      "server_url": "http://localhost:3090",
      "websocket_url": "ws://localhost:3090/vr/websocket",
      "auto_connect": true,
      "heartbeat_interval": 30,
      "reconnect_attempts": 5,
      "reconnect_delay": 5000
    }
  },
  "directories": {
    "projects": "/vr_projects/",
    "published": "/vr/",
    "templates": "/vr_templates/",
    "assets": "/vr_assets/"
  },
  "generation": {
    "openai_model": "gpt-4",
    "max_tokens": 4000,
    "temperature": 0.7,
    "supported_types": [
      "game",
      "experience",
      "scene",
      "environment",
      "interactive"
    ],
    "default_framework": "three.js",
    "fallback_framework": "aframe"
  },
  "deployment": {
    "auto_deploy": true,
    "cloud_platform": "cloud_run",
    "backup_to_gcp": true,
    "cdn_enabled": false,
    "compression": true,
    "core_integration": {
      "enabled": true,
      "server_port": 3090,
      "websocket_port": 3090,
      "auto_start": true,
      "restart_on_failure": true,
      "log_level": "info"
    }
  },
  "ui_integration": {
    "dashboard_section": "VR Studio",
    "show_in_navigation": true,
    "icon": "vr-headset",
    "color_theme": "purple",
    "position": "after_ai_tools"
  },
  "security": {
    "allowed_origins": ["https://*.run.app", "https://localhost:3000"],
    "cors_enabled": true,
    "helmet_security": true,
    "rate_limiting": {
      "enabled": true,
      "requests_per_minute": 60,
      "burst_limit": 10
    }
  },
  "monitoring": {
    "analytics_enabled": true,
    "error_tracking": true,
    "performance_monitoring": true,
    "user_session_tracking": false,
    "core_integration_monitoring": {
      "enabled": true,
      "metrics_collection": true,
      "heartbeat_monitoring": true,
      "error_reporting": true,
      "performance_tracking": true
    }
  },
  "omni_core_integration": {
    "enabled": true,
    "auto_initialize": true,
    "component_registration": {
      "register_vr_devices": true,
      "register_vr_sessions": true,
      "register_vr_projects": true,
      "sync_metrics": true
    },
    "quantum_integration": {
      "enabled": true,
      "enhance_vr_processing": true,
      "quantum_session_optimization": true,
      "ai_assisted_vr": true
    },
    "neural_integration": {
      "enabled": true,
      "adaptive_vr_experiences": true,
      "user_behavior_learning": true,
      "personalized_content": true
    }
  },
  "examples": {
    "trampoline_game": {
      "name": "3D Trampoline VR Game",
      "description": "Interactive trampoline physics game in VR",
      "framework": "three.js",
      "complexity": "intermediate",
      "estimated_generation_time": "30s"
    },
    "art_gallery": {
      "name": "VR Art Gallery",
      "description": "Interactive art gallery experience",
      "framework": "aframe",
      "complexity": "beginner",
      "estimated_generation_time": "20s"
    },
    "meditation_space": {
      "name": "VR Meditation Environment",
      "description": "Peaceful meditation space with ambient sounds",
      "framework": "three.js",
      "complexity": "beginner",
      "estimated_generation_time": "25s"
    }
  }
}